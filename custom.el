(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(fill-column 120)
 '(minimap-major-modes (quote (prog-mode coq-mode tex-mode)))
 '(minimap-minimum-width 10)
 '(minimap-mode t)
 '(minimap-recenter-type (quote free))
 '(minimap-width-fraction 0.2)
 '(minimap-window-location (quote right))
 '(safe-local-variable-values
   (quote
    ((haskell-process-use-ghci . t)
     (haskell-indent-spaces . 4))))
 '(tabbar-background-color nil)
 '(tabbar-mode t nil (tabbar)))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(minimap-active-region-background ((t (:background "dark slate gray"))))
 '(minimap-font-face ((t (:height 35 :family "Liberation Mono")))))
